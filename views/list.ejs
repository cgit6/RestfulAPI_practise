<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>user list</title>
  </head>
  <body>
    <h1>User List</h1>
    <ol>
      <% data.forEach( user => { %>
      <li>
        <a href="/user/<%= user.id %> ">
          <%= user.id%> / <%= user.username %>
        </a>
      </li>
      <% }) %>
    </ol>
  </body>
</html>
